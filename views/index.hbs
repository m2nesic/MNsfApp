<h1>Salesforce Games</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Genre</th>
    <th>Studio</th>
    <th>Release Date</th>
    <th>Picture</th>
  </tr>
  {{#each records}}
    <tr>
      <td>{{get 'Name'}}</td>
      <td>{{get 'Genre__c'}}</td>
      <td>{{get 'Game_studio__r.Name'}}nested json object not showing despite query getting data</td>
      <td>{{get 'Release_Date__c'}}</td>
      <td>{{{get 'TextPicture__c'}}} </td>
    </tr>
    {{else}}
    <tr>
      <td>Not working</td>
    </tr>
  {{/each}}
</table>
